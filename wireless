ifconfig wlan0 down
ifconfig wlan0 up
iwconfig wlan0 essid OTENET_6398
iwconfig wlan0 ap 00:15:56:b6:0d:61
ifconfig wlan0 192.168.2.4 netmask 255.255.255.0 up
route add default gw 192.168.2.1 
echo -e "#generated by /etc/conf.d/net.wlan0.script \nnameserver 192.168.2.1\nnameserver 195.251.25.60\nnameserver 194.177.210.211" > /etc/resolv.conf

